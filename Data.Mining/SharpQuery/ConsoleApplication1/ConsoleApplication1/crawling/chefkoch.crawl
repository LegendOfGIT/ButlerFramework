BaseUri = http://www.chefkoch.de
Source = http://www.chefkoch.de/rs/s0/lachs/Rezepte.html

current.uri >> pageuri
pageuri:browse()
 .rowclick
  .recipe-result-col-1 
   a @href >> rezept.uri   
    [0-9]* >> rezept.id
   img @src >> rezept.bilder
  .search-result-title >> rezept.titel
  rezept.uri:browse()
   .incredients
    .ingredient:not(:first-child)
     .name >> rezept.zutat
     .amount >> rezept.zutat