"http://www.chefkoch.de" >> base.uri
"http://www.chefkoch.de/rs/s0/lachs/Rezepte.html" >> current.uri

current.uri >> pageuri
pageuri:browse()
 .rowclick
  .recipe-result-col-1 
   a @href >> rezept.uri   
    [0-9]* >> rezept.id
   img @src >> rezept.bilder
  .search-result-title >> rezept.titel
  rezept.uri:browse()
   .incredients
   .ingredient:not(:first-child)
    .amount >> rezept.zutat
    .name >> rezept.zutat
  "essen, rezept" >> rezept.stichwort